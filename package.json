{
  "name": "@vricosti/react-native-asset",
  "type": "module",
  "version": "2.1.4",
  "description": "Linking and unlinking of assets in your react-native app, works for fonts and sounds",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "babel lib -d dist --copy-files",
    "prepublishOnly": "yarn build",
    "lint": "eslint --ext .js,.jsx .",
    "lint:error": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --format html -o ./report.html",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vricosti/react-native-asset.git"
  },
  "keywords": [
    "react-native",
    "react",
    "native",
    "js",
    "asset",
    "assets",
    "link"
  ],
  "author": "Yuval Saraf",
  "contributors": [
    {
      "name": "Vince Ricosti"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vricosti/react-native-asset/issues"
  },
  "homepage": "https://github.com/vricosti/react-native-asset#readme",
  "peerDependencies": {
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "fs-extra": "^11.3.0",
    "lodash": "4.17.21",
    "npmlog": "^7.0.1",
    "plist": "^3.1.0",
    "sha1-file": "^3.0.0",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-dynamic-import": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0"
  }
}
